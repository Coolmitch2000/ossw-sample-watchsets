{
	"type": "watchset",
	"name": "weather-time-music",
	"apiVersion": 1,
	"data": {
		"screens": [
			{
			"id": "main-watch-face",
			"controls": [
			{
			"type": "imageFromSet",
			"position": {"x": 90, "y": 1},
			"style": {"width": 32, "height": 32},
			"imageSet": {"type": "resource", "id": "weather"},
			"source": {"type": "extension", "extensionId": "com.klapaucius.ossw.weather", "property": "weatherId"}
			},			
			{
				"choose": {"type": "internal", "property": "dayOfWeek"},
				"when": {
					"1": [ {
						"type": "text", "length": 3,
						"position": {"x": 1, "y": 1},
						"size": {"width": 22, "height": 12},
						"font": {"type": "builtin", "name": "smallRegular"},
						"style": {"horizontalAlign": "right"},
						"source": {"type": "static", "value": "Sun"}
					} ],
					"2": [ {
						"type": "text", "length": 3,
						"position": {"x": 1, "y": 1},
						"size": {"width": 22, "height": 12},
						"font": {"type": "builtin", "name": "smallRegular"},
						"style": {"horizontalAlign": "right"},
						"source": {"type": "static", "value": "Mon"}
					} ],
					"3": [ {
						"type": "text", "length": 3,
						"position": {"x": 1, "y": 1},
						"size": {"width": 22, "height": 12},
						"font": {"type": "builtin", "name": "smallRegular"},
						"style": {"horizontalAlign": "right"},
						"source": {"type": "static", "value": "Tue"}
					} ],
					"4": [ {
						"type": "text", "length": 3,
						"position": {"x": 1, "y": 1},
						"size": {"width": 22, "height": 12},
						"font": {"type": "builtin", "name": "smallRegular"},
						"style": {"horizontalAlign": "right"},
						"source": {"type": "static", "value": "Wed"}
					} ],
					"5": [ {
						"type": "text", "length": 3,
						"position": {"x": 1, "y": 1},
						"size": {"width": 22, "height": 12},
						"font": {"type": "builtin", "name": "smallRegular"},
						"style": {"horizontalAlign": "right"},
						"source": {"type": "static", "value": "Thu"}
					} ],
					"6": [ {
						"type": "text", "length": 3,
						"position": {"x": 1, "y": 1},
						"size": {"width": 22, "height": 12},
						"font": {"type": "builtin", "name": "smallRegular"},
						"style": {"horizontalAlign": "right"},
						"source": {"type": "static", "value": "Fri"}
					} ],
					"7": [ {
						"type": "text", "length": 3,
						"position": {"x": 1, "y": 1},
						"size": {"width": 22, "height": 12},
						"font": {"type": "builtin", "name": "smallRegular"},
						"style": {"horizontalAlign": "right"},
						"source": {"type": "static", "value": "Sat"}
					} ]
				}
			},
			{
			"type": "number",
			"numberRange": "0-99",
			"position": {"x": 29, "y": 2},
			"style": {"type": "generated", "thickness": 1, "width": 5, "height": 8, "space": 1, "leftPadded": true},
			"source": {"type": "internal", "property": "dayOfMonth"}
			},
			{
				"choose": {"type": "internal", "property": "month"},
				"when": {
					"1": [ {
						"type": "text", "length": 3,
						"position": {"x": 46, "y": 1},
						"size": {"width": 22, "height": 12},
						"font": {"type": "builtin", "name": "smallRegular"},
						"style": {"horizontalAlign": "left"},
						"source": {"type": "static", "value": "Jan"}
					} ],
					"2": [ {
						"type": "text", "length": 3,
						"position": {"x": 46, "y": 1},
						"size": {"width": 22, "height": 12},
						"font": {"type": "builtin", "name": "smallRegular"},
						"style": {"horizontalAlign": "left"},
						"source": {"type": "static", "value": "Feb"}
					} ],
					"3": [ {
						"type": "text", "length": 3,
						"position": {"x": 46, "y": 1},
						"size": {"width": 22, "height": 12},
						"font": {"type": "builtin", "name": "smallRegular"},
						"style": {"horizontalAlign": "left"},
						"source": {"type": "static", "value": "Mar"}
					} ],
					"4": [ {
						"type": "text", "length": 3,
						"position": {"x": 46, "y": 1},
						"size": {"width": 22, "height": 12},
						"font": {"type": "builtin", "name": "smallRegular"},
						"style": {"horizontalAlign": "left"},
						"source": {"type": "static", "value": "Apr"}
					} ],
					"5": [ {
						"type": "text", "length": 3,
						"position": {"x": 46, "y": 1},
						"size": {"width": 22, "height": 12},
						"font": {"type": "builtin", "name": "smallRegular"},
						"style": {"horizontalAlign": "left"},
						"source": {"type": "static", "value": "May"}
					} ],
					"6": [ {
						"type": "text", "length": 3,
						"position": {"x": 46, "y": 1},
						"size": {"width": 22, "height": 12},
						"font": {"type": "builtin", "name": "smallRegular"},
						"style": {"horizontalAlign": "left"},
						"source": {"type": "static", "value": "Jun"}
					} ],
					"7": [ {
						"type": "text", "length": 3,
						"position": {"x": 46, "y": 1},
						"size": {"width": 22, "height": 12},
						"font": {"type": "builtin", "name": "smallRegular"},
						"style": {"horizontalAlign": "left"},
						"source": {"type": "static", "value": "Jul"}
					} ],
					"8": [ {
						"type": "text", "length": 3,
						"position": {"x": 46, "y": 1},
						"size": {"width": 22, "height": 12},
						"font": {"type": "builtin", "name": "smallRegular"},
						"style": {"horizontalAlign": "left"},
						"source": {"type": "static", "value": "Aug"}
					} ],
					"9": [ {
						"type": "text", "length": 3,
						"position": {"x": 46, "y": 1},
						"size": {"width": 22, "height": 12},
						"font": {"type": "builtin", "name": "smallRegular"},
						"style": {"horizontalAlign": "left"},
						"source": {"type": "static", "value": "Sep"}
					} ],
					"10": [ {
						"type": "text", "length": 3,
						"position": {"x": 46, "y": 1},
						"size": {"width": 22, "height": 12},
						"font": {"type": "builtin", "name": "smallRegular"},
						"style": {"horizontalAlign": "left"},
						"source": {"type": "static", "value": "Oct"}
					} ],
					"11": [ {
						"type": "text", "length": 3,
						"position": {"x": 46, "y": 1},
						"size": {"width": 22, "height": 12},
						"font": {"type": "builtin", "name": "smallRegular"},
						"style": {"horizontalAlign": "left"},
						"source": {"type": "static", "value": "Nov"}
					} ],
					"12": [ {
						"type": "text", "length": 3,
						"position": {"x": 46, "y": 1},
						"size": {"width": 22, "height": 12},
						"font": {"type": "builtin", "name": "smallRegular"},
						"style": {"horizontalAlign": "left"},
						"source": {"type": "static", "value": "Dec"}
					} ]
				}
			},
			{
			"type": "text", "length": 6,
			"position": {"x": 0, "y": 14},
			"size": {"width": 70, "height": 20},
			"font": {"type": "builtin", "name": "optionBig"},
			"style": {"horizontalAlign": "center"},
			"source": {"type": "extension", "extensionId": "com.klapaucius.ossw.weather", "property": "temperature"}
			},
			{
			"type": "text", "length": 4,
			"position": {"x": 5, "y": 38},
			"size": {"width": 30, "height": 12},
			"font": {"type": "builtin", "name": "smallRegular"},
			"style": {"horizontalAlign": "left"},
			"source": {"type": "extension", "extensionId": "com.klapaucius.ossw.weather", "property": "humidity"}
			},
			{
			"type": "text", "length": 5,
			"position": {"x": 34, "y": 38},
			"size": {"width": 35, "height": 12},
			"font": {"type": "builtin", "name": "smallRegular"},
			"style": {"horizontalAlign": "left"},
			"source": {"type": "extension", "extensionId": "com.klapaucius.ossw.weather", "property": "windSpeed"}
			},
			{
			"type": "text", "length": 30,
			"position": {"x": 70, "y": 33},
			"size": {"width": 74, "height": 30},
			"font": {"type": "builtin", "name": "smallRegular"},
			"style": {"horizontalAlign": "center", "multiline": "true"},
			"source": {"type": "extension", "extensionId": "com.klapaucius.ossw.weather", "property": "weatherDescription"}
			},
			{
			"type": "number",
			"numberRange": "0-99",
			"position": {"x": 4, "y": 64},
			"style": {"type": "generated", "thickness": 6, "width": 30, "height": 50, "space": 4, "leftPadded": true},
			"source": {"type": "internal", "property": "hour"}
			},
			{
			"type": "number",
			"numberRange": "0-99",
			"position": {"x": 76, "y": 64},
			"style": {"type": "generated", "thickness": 6, "width": 30, "height": 50, "space": 4, "leftPadded": true},
			"source": {"type": "internal", "property": "minutes"}
			},
			{
			"type": "number",
			"numberRange": "0-999",
			"position": {"x": 129, "y": 1},
			"style": {"type": "generated", "thickness": 1, "width": 4, "height": 6, "space": 1, "leftPadded": false},
			"source": {"type": "internal", "property": "batteryLevel"}
			},
			{
			 "type": "text", "length": 60,
			 "position": {"x": 0, "y": 120},
			 "size": {"width": 144, "height": 24},
			 "font": {"type": "builtin", "name": "smallRegular"},
			 "style": {"horizontalAlign": "center", "multiline": "true"},
			 "source": {"type": "extension", "extensionId": "com.althink.android.ossw.plugins.musicplayer", "property": "track"}
			},
			{
	 		"type": "text", "length": 30,
	 		"position": {"x": 0, "y": 144},
			"size": {"width": 144, "height": 12},
	 		"font": {"type": "builtin", "name": "smallRegular"},
			"style": {"horizontalAlign": "center"},
			"source": {"type": "extension", "extensionId": "com.althink.android.ossw.plugins.musicplayer", "property": "album"}
		 	},
			{
	 		"type": "text", "length": 30,
	 		"position": {"x": 0, "y": 156},
	 		"size": {"width": 144, "height": 12},
			"font": {"type": "builtin", "name": "smallRegular"},
			"style": {"horizontalAlign": "center"},
			"source": {"type": "extension", "extensionId": "com.althink.android.ossw.plugins.musicplayer", "property": "artist"}
			}
			],
			"actions": {
				"button_up_short": {"action": "extensionFunction",  "extensionId": "com.althink.android.ossw.plugins.musicplayer", "function": "prevTrack"},
				"button_select_short": {"action": "extensionFunction",  "extensionId": "com.althink.android.ossw.plugins.musicplayer", "function": "playPause"},
				"button_select_long": {"action": "settings"},
				"button_back_short": {"action": "nextWatchface"},
				"button_back_long": {"action": "close"},
				"button_down_short": {"action": "extensionFunction",  "extensionId": "com.althink.android.ossw.plugins.musicplayer", "function": "nextTrack"},
				"button_down_long": {"action": "extensionFunction", "extensionId": "com.althink.android.ossw", "function": "phone.discovery"}
			}
			}
		],
		"resources": [
		{
		"id": "weather",
		"data": ""
		}
		]
	}
}
